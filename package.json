  },
  "homepage": "https://github.com/primo19/graphql-vue-template#readme",
  "dependencies": {
    "apollo-server": "^2.9.14",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongoose": "^5.8.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
